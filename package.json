  },
  "bugs": {
    "url": "https://bitbucket.org/hragency_back/hragency_back_repo/issues"
  },
  "scripts": {
    "start": "node index.js",
    "watch": "nodemon index.js",
    "test": "mocha 'test/**/*.test.js' --exit --bail",
    "test:g": "mocha 'test/**/*.test.js' --exit --grep",
    "test:i": "dev/test-runner.sh start && mocha 'test/**/*.it.js' --exit --bail && dev/test-runner.sh",
    "test:i:g": "dev/test-runner.sh start && mocha 'test/**/*.it.js' --exit --grep && dev/test-runner.sh",
    "startenv": "dev/test-runner.sh start",
    "stopenv": "dev/test-runner.sh stop"
  },
  "nodemonConfig": {
    "ext": "*.js, *.json",
    "ignore": [
      "log/*",
      "test/*",
      "db/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-prom-bundle": "^8.0.0",
    "fs": "latest",
    "http": "latest",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.7.0",
    "joi": "^17.1.1",
    "joi-to-swagger": "^6.2.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "kafkajs-snappy": "^1.1.0",
    "knex": "^3.1.0",
    "mime": "^4.0.7",
    "moment": "^2.30.1",
    "nconf": "^0.12.1",
    "nodejs": "latest",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "passport": "latest",
    "passport-jwt": "latest",
    "passport-local": "latest",
    "path": "latest",
    "pg": "^8.14.1",
    "supertest": "^7.1.0",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.8.3",
    "url": "latest",
    "uuid": "latest",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@tsconfig/node23": "^23.0.1",
