  "scripts": {
    "start": "node index.js",
    "watch": "nodemon index.js",
    "test": "mocha 'test/**/*.test.js' --bail",
    "test:g": "mocha 'test/**/*.test.js' --grep",
    "test:i": "dev/test-runner.sh start && mocha 'test/**/*.it.js' --bail && dev/test-runner.sh",
    "test:i:g": "dev/test-runner.sh start && mocha 'test/**/*.it.js' --grep && dev/test-runner.sh",
    "startenv": "dev/test-runner.sh start",
    "stopenv": "dev/test-runner.sh stop"
  },
  "nodemonConfig": {
    "ext": "*.js, *.json",
    "ignore": [
      "log/*",
      "test/*",
      "db/*"
    ],
    "delay": "2500"
  },
